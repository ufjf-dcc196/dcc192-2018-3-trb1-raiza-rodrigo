<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raiza.semanacomputacao">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Semana da Computação"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/SemanaComputacao">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListarPtcActivity" />
        <activity android:name=".CadastrarParticipanteActivity" />
        <activity android:name=".CadastrarEventoActivity" />
        <activity android:name=".ListarEvtActivity" />
        <activity android:name=".EditarParticipanteActivity" />
        <activity android:name=".InscreverParticipanteEventoActivity" />
        <activity android:name=".SelecionaEventoActivity"></activity>
    </application>

</manifest>